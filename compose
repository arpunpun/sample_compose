>kiosk_adminpanel

version: '3'

services:

  kiosk-adminpanel:
    image: sc-harbor.suncity-group.com/loyalty/kiosk_admin_web:1.0.0-build.59
    hostname: '{{.Node.Hostname}}'
    deploy:
      mode: replicated
      placement:
       constraints:
         #- node.role == worker
         - node.labels.type == node
      replicas: 1
      restart_policy:
        condition: on-failure
        delay: 5s
        #max_attempts: 10
      update_config:
        parallelism: 1
        delay: 20s
    ports:
      - 8080:3000
    restart: always

