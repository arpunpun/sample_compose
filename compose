#SERVICES

----------------------------------------------------------------------------------------------------------

>minio

version: '2'

services:
  minio:
    image: minio/minio:latest
    container_name: minio
    hostname: minio
    restart: always
    command: server /data1 /data2 /data3 /data4 /data5 /data6 /data7 /data8
    environment:
      - MINIO_ACCESS_KEY=FR2QXA7Y
      - MINIO_SECRET_KEY=AMFJZTX92CB8NS8U
    volumes:
      - "/home/root/docker/minio/minio/data/data1:/data1"
      - "/home/root/docker/minio/minio/data/data2:/data2"
      - "/home/root/docker/minio/minio/data/data3:/data3"
      - "/home/root/docker/minio/minio/data/data4:/data4"
      - "/home/root/docker/minio/minio/data/data5:/data5"
      - "/home/root/docker/minio/minio/data/data6:/data6"
      - "/home/root/docker/minio/minio/data/data7:/data7"
      - "/home/root/docker/minio/minio/data/data8:/data8"
      - "/home/root/docker/minio/minio/data/config:/root/.minio"
    ports:
      - "9001:9000"

----------------------------------------------------------------------------------------------------------

>redis

version: '2'
services:
  redis:
    image: redis:6.2.4
    container_name: redis
    command: redis-server /etc/redis/redis.conf
    ports:
      - 6379:6379
    hostname: redis
    restart: always
    volumes:
      - "/home/root/docker/redis/data:/data"
      - "/home/root/docker/redis/redis.conf:/etc/redis/redis.conf"

----------------------------------------------------------------------------------------------------------

>matomo

version: '2'

services:
  matomo:
    #image: bitnami/matomo:3.11.0
    image: matomo:3.11.0
    container_name: matomo
    hostname: matomo
    restart: always
    environment:
      - "MATOMO_USERNAME=root"
      - "MATOMO_PASSWORD=bitnami"
      - "MATOMO_DATABASE_HOST=mysql"
      - "MATOMO_DATABASE_USERNAME=root"
      - "MATOMO_DATABASE_PASSWORD=root"
      - "MATOMO_DATABASE_DBNAME=matomo"

    ports:
      - "80:80"
      #- "443:30081"
      
  mysql:
    image: mysql:5.7
    container_name: mysql
    hostname: mysql
    restart: always
    environment:
      - "MYSQL_ROOT_PASSWORD=root"
      - "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
      - "GOSU_VERSION=1.7"
      - "MYSQL_MAJOR=5.7"
      - "MYSQL_VERSION=5.7.28-1debian9"
    volumes:
      - "/home/root/docker/mysql/config:/etc/mysql/conf.d"
      - "/home/root/docker/mysql/data:/var/lib/mysql"
    ports:
      - "3306:3306"

----------------------------------------------------------------------------------------------------------

>rabbitmq

version: '3'

services:
  rabbitmq:
    image: 'rabbitmq:3.8.9-management'
    hostname: rabbitmq
    container_name: rabbitmq
    environment:
      RABBITMQ_DEFAULT_USER: admin
      RABBITMQ_DEFAULT_PASS: fXRSLmpvtPEGmmH2FsjuumsagpvYJBcQ
    ports:
      - '5672:5672'
      - '15672:15672'
      # - '15674:15674'
    volumes:
      - '/data/rabbitmq:/var/lib/rabbitmq'
    restart: always

